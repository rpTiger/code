//menus
add_theme_support( 'menus' );


function wpb_custom_new_menu() {
  register_nav_menus(
    array(
      'about-menu' => __( 'About Menu' ),
      'top-menu' => __( 'Top Menu' ),
      'footer-menu' => __( 'Footer Menu' ),
      'Center1' => __( 'Center 1' ),
      'Center2' => __( 'Center 2' )
    )
  );
}
add_action( 'init', 'wpb_custom_new_menu' );


//menus
